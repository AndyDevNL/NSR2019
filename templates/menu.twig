{% if PrimaryMenu %}
    <ul class="PrimaryMenu header__element">
    {% for item in PrimaryMenu.items %}
          
      <li class="PrimaryMenu__Item PrimaryMenu__NavItem {{ item.classes | join(' ') }}">
        <a class="link" target="{{ item.target }}" href="{{ item.link }}">{{ item.title }}</a>
         {% if item.children %}
              <ul class="PrimaryMenu__dropMenu hiddenMenu">
                  {% for child in item.children %}
                      <li class="PrimaryMenu__dropItem">
                          <a class="dropLink" href="{{ child.link }}">{{ child.title }}</a>
                      </li>
                  {% endfor %}
              </ul>
          {% endif %}
      </li>

    {% endfor %}
    </ul>
{% endif %}

