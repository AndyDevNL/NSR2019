<!doctype html>
<html class="no-js no-svg" {{ site.language_attributes }}>
{% block site_head %}
{% include 'html-header.twig' %}
{% endblock %}

	<body class="{{body_class}}" data-template="base.twig">
    {% block header %}
      {% include 'partial/header.twig' %}
    {% endblock %}
    
    <div id="content" class="content" role="main">
      <div class="container">
        {% block content %}
          
        {% endblock %}
      </div>
    </div>

    {% include 'partial/footer.twig' %}

    <script src="{{ site.theme.link }}/jquery.js"type="text/javascript"></script>
    <script src="{{ site.theme.link }}/NSR2019.js"type="text/javascript"></script>
    
	</body>

</html>
